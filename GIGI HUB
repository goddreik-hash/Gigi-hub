        b.Size,b.Position,b.AnchorPoint,b.BackgroundColor3,b.Text,b.TextColor3,b.Font,b.TextSize=UDim2.new(0,150,0,35),UDim2.new(0.5,0,0.25+(i-1)*0.2,0),Vector2.new(0.5,0),_BC,n,_TC,Enum.Font.GothamBold,16
