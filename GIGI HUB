    _A2.InputChanged:Connect(function(i) if _D and i.UserInputType==Enum.UserInputType.MouseMovement then local d=i.Position-_S _F.Position=UDim2.new(_P.X.Scale,_P.X.Offset+d.X,_P.Y.Scale,_P.Y.Offset+d.Y) end end)
    _ML.InputBegan:Connect(function(i) end) -- fake hook

    -- botones principales con ruido
    local _BTN_NAMES={"Block Drop","Elemental Hub","Permadeath","Reset"}
    local _BTN_FUNCS={
        function() loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerReady/Block-Drop/main/BlockDrop.lua",true))() end,
