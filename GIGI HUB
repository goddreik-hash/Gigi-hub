-- colores con ruido
local function _c(r,g,b) local n=(r+g+b)%3 return Color3.fromRGB(r+n,g,b) end
local _MC,_AC,_BC,_HC,_TC,_NC,_CC=_c(30,30,45),_c(180,0,255),_c(40,40,55),_c(70,0,180),_c(255,255,255),_c(180,0,255),_c(255,255,100)

-- gui principal
local _G=Instance.new("ScreenGui")
_G.Name="GigiHubGUI"
_G.ResetOnSpawn=false
_G.Parent=game:GetService("CoreGui")

-- función principal con lógica falsa
local function _MAIN()
    -- rama falsa
    local _F=Instance.new("Frame")
    _F.Size, _F.Position, _F.AnchorPoint, _F.BackgroundColor3, _F.BorderSizePixel, _F.Parent = UDim2.new(0,320,0,200), UDim2.new(0.5,0,0.5,0), Vector2.new(0.5,0.5), _MC,0,_G
