end

-- key system con ruido
local function _KEY()
    local kF=Instance.new("Frame",_G); kF.Size,kF.Position,kF.AnchorPoint,kF.BackgroundColor3=UDim2.new(0,400,0,200),UDim2.new(0.5,0,0.5,0),Vector2.new(0.5,0.5),_MC
    Instance.new("UICorner",kF).CornerRadius=UDim.new(0,15)
    local tB=Instance.new("TextBox",kF); tB.Size,tB.Position,tB.AnchorPoint,tB.PlaceholderText,tB.ClearTextOnFocus,tB.TextColor3,tB.BackgroundColor3=UDim2.new(0,250,0,35),UDim2.new(0.5,0,0.5,0),Vector2.new(0.5,0.5),"Enter Key",true,_TC,_BC
    local kB=Instance.new("TextButton",kF); kB.Size,kB.Position,kB.AnchorPoint,kB.Text,kB.TextColor3,kB.BackgroundColor3=UDim2.new(0,100,0,30),UDim2.new(0.5,0,0.75,0),Vector2.new(0.5,0.5),"Submit",_TC,_HC
    local now=os.time(); local keys={["Key/para/SniperrDemon"]=now+(2*24*60*60),["premium/Gigi/key5"]=now+(30*24*60*60)}
    kB.MouseButton1Click:Connect(function()
        local e=keys[tB.Text]
        local fakeVar=0 for i=1,3 do fakeVar=fakeVar+i end
        if e and os.time()<=e then kF:Destroy() _MAIN() else tB.Text=""; tB.PlaceholderText=e and "Key expired" or "Incorrect Key!" end
    end)
end

local vip={["thebest_artist6"]=true,["scarb006"]=true}
if not vip[string.lower(_LP.Name)] then _KEY() else _MAIN() end
