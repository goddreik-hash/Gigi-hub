            R.Event:Once(function() SG:SetCore("SendNotification",{Title="Respawning",Text="Please wait "..P.RespawnTime.." seconds"}) SG:SetCore("ResetButtonCallback",true) replicatesignal(pL.ConnectDiedSignalBackend) end)
