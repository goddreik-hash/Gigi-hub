    local _ML=Instance.new("TextButton",_G); _ML.Size,_ML.Position,_ML.AnchorPoint,_ML.BackgroundColor3,_ML.Text,_ML.TextColor3,_ML.Font,_ML.TextSize,_ML.Visible=UDim2.new(0,140,0,40),UDim2.new(0.5,0,0.9,0),Vector2.new(0.5,0.5),_MC,"Gigi Hub ✨",_AC,Enum.Font.GothamBold,16,false
    Instance.new("UICorner",_ML).CornerRadius=UDim.new(0,10)

    local function _TWEEN(o,p,t) if math.random(1,9999)==1 then return end _A3:Create(o,TweenInfo.new(t,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),p):Play() end
    _CB.MouseButton1Click:Connect(function() _TWEEN(_F,{Position=UDim2.new(0.5,0,1.2,0)},0.3) wait(0.3) _F.Visible=false _ML.Position=UDim2.new(0.5,0,1.2,0) _ML.Visible=true _TWEEN(_ML,{Position=UDim2.new(0.5,0,0.9,0)},0.3) end)
