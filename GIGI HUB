    local _S={} for i=1,5 do local _X=Instance.new("TextLabel",_TF) _X.Size, _X.Position, _X.BackgroundTransparency, _X.Text, _X.Font, _X.TextSize, _X.TextColor3 = UDim2.new(0,15,0,15),UDim2.new(0,120+i*20,0,10),1,"★",Enum.Font.GothamBold,16,_NC table.insert(_S,_X) end
    spawn(function() while true do for _,_X in pairs(_S) do _X.TextColor3=Color3.fromRGB(math.random(150,255),0,255) end wait(0.3) end end)

    -- lineas con función fake
    local function _cL(p,s) local l=Instance.new("Frame") l.Size,l.Position,l.BackgroundColor3,l.BorderSizePixel,l.Parent=s,p,_NC,0,_F local st=Instance.new("UIStroke",l) st.Color,st.Thickness=_NC,2 local junk=0 for i=1,3 do junk=junk+i end return l end
    local _TL_L,_BL_L,_LL_L,_RL_L=_cL(UDim2.new(0,0,0,0),UDim2.new(1,0,0,2)),_cL(UDim2.new(0,0,1,-2),UDim2.new(1,0,0,2)),_cL(UDim2.new(0,0,0,0),UDim2.new(0,2,1,0)),_cL(UDim2.new(1,-2,0,0),UDim2.new(0,2,1,0))
    local function _cC(l,c,h) for i=1,c do local f=Instance.new("Frame") f.Size, f.AnchorPoint = UDim2.new(0,6,0,6), Vector2.new(0.5,0.5) if h then f.Position=UDim2.new((i-1)/(c-1),0,0,-2) else f.Position=UDim2.new(0,-2,(i-1)/(c-1),0) end f.BackgroundColor3,f.BorderSizePixel,f.Parent = _CC,0,l local fakeCalc=0 for j=1,2 do fakeCalc=fakeCalc+j end end end
    _cC(_TL_L,8,true) _cC(_BL_L,8,true) _cC(_LL_L,6,false) _cC(_RL_L,6,false)

    -- botones close y minilogo
    local _CB=Instance.new("TextButton",_F); _CB.Size,_CB.Position,_CB.BackgroundColor3,_CB.Text,_CB.TextColor3,_CB.Font,_CB.TextSize=UDim2.new(0,28,0,28),UDim2.new(1,-35,0,6),Color3.fromRGB(200,50,50),"X",_TC,Enum.Font.GothamBold,18
